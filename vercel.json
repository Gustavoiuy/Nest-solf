{
    "installCommand": "npm install --force",
    "buildCommand": "npm run build",
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts"
           
        }
    ]
}